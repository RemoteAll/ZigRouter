{
  "version": "1.0.0",

  "// 服务器配置": "",
  "server_addr": "127.0.0.1",
  "server_port": 7891,

  "// 本机信息": "",
  "machine_name": "",
  "machine_id": "",

  "// 连接设置": "",
  "heartbeat_interval": 30,
  "connect_timeout": 10,
  "auto_reconnect": true,
  "reconnect_interval": 5,

  "// NAT 检测设置": "",
  "auto_detect_nat": true,
  "stun_server": "stun.l.google.com",
  "stun_port": 19302,
  "port_map_wan": 0,

  "// 日志设置": "",
  "log_level": "info",
  "log_color": true,

  "// TLS 加密设置 (公网部署建议启用)": "",
  "tls": {
    "enabled": true,
    "verify_server": false,
    "allow_self_signed": true,
    "ca_cert_file": "",
    "client_cert_file": "",
    "client_key_file": ""
  },

  "// 打洞方式配置 (按优先级排序，数字越小越优先)": "",
  "transports": [
    {
      "name": "UdpPortMap",
      "enabled": true,
      "priority": 1,
      "timeout_seconds": 10,
      "retry_count": 3
    },
    {
      "name": "TcpPortMap",
      "enabled": true,
      "priority": 2,
      "timeout_seconds": 10,
      "retry_count": 3
    },
    {
      "name": "Udp",
      "enabled": true,
      "priority": 3,
      "timeout_seconds": 10,
      "retry_count": 3
    },
    {
      "name": "UdpP2PNAT",
      "enabled": true,
      "priority": 4,
      "timeout_seconds": 15,
      "retry_count": 3
    },
    {
      "name": "TcpP2PNAT",
      "enabled": true,
      "priority": 5,
      "timeout_seconds": 15,
      "retry_count": 3
    },
    {
      "name": "TcpNutssb",
      "enabled": true,
      "priority": 6,
      "timeout_seconds": 15,
      "retry_count": 3
    },
    {
      "name": "MsQuic",
      "enabled": false,
      "priority": 7,
      "timeout_seconds": 10,
      "retry_count": 2
    }
  ]
}
